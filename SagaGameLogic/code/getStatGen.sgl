#include <stdlib.h>
#include <fstream>
#include <iostream>
using namespace std;

int main() {
	ifstream in("stat.sgl");
	ofstream dec("getDec.sgl");
	ofstream def("getDef.sgl");
	string stat, aux;
	while(!in.eof()) {
		in>>stat;
		stat.pop_back();
		if(stat.empty()) break;
		aux=stat;
		aux.at(0)+='A'-'a';
		dec<<"StatValue get"<<aux<<"() const;"<<endl;
		def<<"StatValue Character::get"<<aux<<"() const {"<<endl<<
			"\treturn "<<stat<<";"<<endl<<"}"<<endl;
	}
	return 0;
}